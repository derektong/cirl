<%= provide( :title, 'Manage Keywords') %>
<section class="row">
<%= render 'shared/admin_menu' %>
<div class="span4 basic">
  <h3>Name your search</h3><br />
<%= form_for(@case_search, :html => { :class => "well" } ) do |f| %>
  <%= render "shared/error_messages", :object => f.object %>
  <%= f.label :Name, "Name: " %>
  <%= f.text_field :name %>
  <%= f.submit "Save", class: "btn btn-primary white" %>
  <%= f.submit "Cancel", class: "btn btn-primary white" %>
<% end %>

</div>
</section>
